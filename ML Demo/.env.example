# Azure Functions Configuration
AzureWebJobsStorage=DefaultEndpointsProtocol=https;AccountName=yourstorageaccount;AccountKey=yourstoragekey;EndpointSuffix=core.windows.net
FUNCTIONS_WORKER_RUNTIME=python

# Azure ML Endpoints and Keys
AZURE_ML_PREDICTION_ENDPOINT=https://your-ml-endpoint.com/score
AZURE_ML_KEY=your-ml-auth-key
AZURE_ML_TRAINING_ENDPOINT=https://your-ml-endpoint.com/train

# Event Hub Configuration
EventHubConnectionString=Endpoint=sb://your-eventhub.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=yoursharedaccesskey=
ALPHABET_EVENT_HUB=alphabet-topic
PREDICTIONS_EVENT_HUB=predictions-topic
CONSUMER_GROUP=$Default

# Azure Blob Storage
AZURE_BLOB_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=yourstorageaccount;AccountKey=yourstoragekey;EndpointSuffix=core.windows.net

# Vision Services
VISION_PREDICTION_KEY=your-vision-prediction-key
VISION_PREDICTION_ENDPOINT=https://your-region.api.cognitive.microsoft.com/
VISION_PREDICTION_RESOURCE_ID=/subscriptions/your-subscription-id/resourceGroups/your-resource-group/providers/Microsoft.CognitiveServices/accounts/your-cognitive-resource
CustomVisionProjectId=your-project-id
CustomVisionPublishedName=your-published-name
